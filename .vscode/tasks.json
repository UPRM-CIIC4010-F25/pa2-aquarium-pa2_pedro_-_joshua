{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "make",
      "args": ["-j6"],
      "options": { "cwd": "${workspaceFolder}" },
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "RunRelease",
      "type": "shell",
      "command": "make",
      "args": ["RunRelease"],
      "options": { "cwd": "${workspaceFolder}" }
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "make",
      "args": ["clean"],
      "options": { "cwd": "${workspaceFolder}" }
    }
  ]
}
